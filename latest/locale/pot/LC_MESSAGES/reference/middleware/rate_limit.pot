msgid "Examples"
msgstr ""

#: ../../../../litestar/middleware/rate_limit.py:docstring of
#: litestar.middleware.rate_limit.RateLimitConfig.middleware:6
msgid ""
"from litestar import Litestar, Request, get\n"
"from litestar.middleware.rate_limit import RateLimitConfig\n"
"\n"
"# limit to 10 requests per minute, excluding the schema path\n"
"throttle_config = RateLimitConfig(rate_limit=(\"minute\", 10), exclude=[\"/"
"schema\"])\n"
"\n"
"\n"
"@get(\"/\")\n"
"def my_handler(request: Request) -> None: ...\n"
"\n"
"\n"
"app = Litestar(route_handlers=[my_handler], middleware=[throttle_config."
"middleware])"
msgstr ""

