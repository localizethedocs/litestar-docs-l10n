#: ../../../index.rst:86
msgid "First, create a file named ``app.py`` with the following contents:"
msgstr ""

#: ../../../index.rst:89
msgid ""
"from litestar import Litestar, get\n"
"\n"
"\n"
"@get(\"/\")\n"
"async def index() -> str:\n"
"    return \"Hello, world!\"\n"
"\n"
"\n"
"@get(\"/books/{book_id:int}\")\n"
"async def get_book(book_id: int) -> dict[str, int]:\n"
"    return {\"book_id\": book_id}\n"
"\n"
"\n"
"app = Litestar([index, get_book])"
msgstr ""

#: ../../../index.rst:105
msgid "Then, run the following command:"
msgstr ""

#: ../../../index.rst:107
msgid ""
"litestar run\n"
"# Or you can run Uvicorn directly:\n"
"uvicorn app:app --reload"
msgstr ""

